{
    "extends": "../tsproject.json",
    "compilerOptions": {
        "outDir": "../dist",
        "rootDir": ".",
        "module": "ES2020",
        "target": "ES2017",
        "lib": ["es2017", "DOM", "ES2019.Object"],
        "moduleResolution": "node",
        "baseUrl": ".",
        "paths": {
            "@keycloakify/angular/*": ["./*"]
        },
        "jsx": "react-jsx",
        "allowSyntheticDefaultImports": true,
        "experimentalDecorators": true
    }
}
